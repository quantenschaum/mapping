          "fill-opacity": 0.1
        }
    },
    {
        "id": "TSS-separation-lane",
        "type": "fill",
        "source": "bsh",
        "source-layer": "Hydrography",
        "filter": [ "all", ["==", ["geometry-type"], "Polygon"],["has","CATTSS"],["==",["get","zone_type"],9]],
        "paint": {
          "fill-color": "yellow",
          "fill-opacity": 0.05

        }
    },
    {
        "id": "TSS-separation-boundary",
        "type": "line",
        "source": "bsh",
        "source-layer": "Hydrography",
        "filter": [ "all", ["==", ["geometry-type"], "LineString"],["has","CATTSS"],["==",["get","zone_type"],7]],
        "paint": {

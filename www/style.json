            ["==",["get","WATLEV"],4],"covers",
            ["==",["get","WATLEV"],5],"awash",
            "submerged"]],
         "icon-overlap": "always",
         "text-field": "({VALSOU})",
         "text-size": ["case",["has","VALSOU"],12,0],
         "text-anchor": "top",
         "text-optional": true,
         "text-offset": [0,0.6]
        },
        "paint": {
          "text-color": ["case", ["<",5,["get","VALSOU"]],"gray", "black"]
        }
    },
    {
        "id": "obstructions",
        "type": "symbol",
        "source": "bsh",
        "source-layer": "RocksWrecksObstructions",
        "filter": [ "all", ["==", ["geometry-type"], "Point"],["!=", ["get","caution_type"], 6]],

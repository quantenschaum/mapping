           ["==",["get","CATWRK"],5],"wreck-hull_showing",
           ["==",["get","CATOBS"],6],"foul",
           ["==",["get","CATOBS"],7],"foul",
           "obstruction"],
         "icon-overlap": "always",
         "text-field": "({VALSOU})",
         "text-size": ["case",["has","VALSOU"],12,0],
         "text-anchor": "top",
         "text-optional": true,
         "text-offset": [0,0.8]
        },
        "paint": {
          "text-color": ["case", ["<",5,["get","VALSOU"]],"gray", "black"]
        }
    },
    {

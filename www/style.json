        "source": "bsh",
        "source-layer": "Hydrography",
        "filter": [ "all", ["==", ["geometry-type"], "Polygon"],["==",["get","zone_type"],7]],
        "paint": {
          "fill-color": "yellow",
          "fill-opacity": 0.05
        }
    },
    {
